{
  "name": "npm-react-boilerplate",
  "version": "0.0.1",
  "description": "npm react package boilerplate",
  "repository": "https://github.com/Tomekmularczyk/react-package-starter",
  "main": "lib/index.js",
  "keywords": [
    "test"
  ],
  "files": [
    "lib"
  ],
  "scripts": {
    "prepare": "webpack --config config/dist.config.js --env.NODE_ENV=production",
    "dev": "webpack --config config/dist.config.js --progress --colors --watch --env.NODE_ENV=development",
    "lint": "standard"
  },
  "pre-push": [
    "lint"
  ],
  "author": "Tomek",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "pre-push": "^0.1.1",
    "react": "^16.2.0",
    "standard": "^11.0.0",
    "webpack": "^4.1.0",
    "webpack-cli": "^2.0.10"
  },
  "peerDependencies": {
    "react": "^16.x.x"
  },
  "babel": {
    "presets": [
      "react",
      "env"
    ]
  },
  "standard": {
    "ignore": [
      "/lib/**"
    ]
  }
}
